<% if flash[:errors] %>
    <%flash[:errors].map do |error|%>
        <%=error%> <div>
    <%end%>
<%end%>

<%= form_for @blogger do |f| %>
<%=f.label :name%>
<%=f.text_field :name%> <div>
<%=f.label :age %>
<%=f.number_field :age %> <div>
<%=f.label :bio%>
<%=f.text_area :bio%> <div>
<%= f.submit %> <div>
<%end%>